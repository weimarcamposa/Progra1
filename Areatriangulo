#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	float base,altura,area;

	cout<<"ingrese base: ";
	cin>>base;
	cout<<"ingrese altura: ";
	cin>>altura;
	area=base*altura;
	cout<<"el area es: "<< area;
	getch();
}
